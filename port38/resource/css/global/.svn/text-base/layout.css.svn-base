#funcBlock{margin-top:15px;}
/*******************************  底部功能块容器 End  **********************************/

/*******************************  左侧菜单栏容器 start  ********************************/
#wraper_leftMenu{
	float:left;
	width:200px;
	background:url(../../image/menu/m05_top.png) 0px 0px no-repeat;
	_background:url(../../image/menu/m05_top.gif) 0px 0px no-repeat;
	min-height:400px;
	_height:400px;
	_heigth:auto !important;
}
.title_leftMenu{
	font-family:"黑体";
	font-size:18px;
	color:#FFF;
	font-weight:bold;
	margin-top:51px;
	display:block;
	margin-left:16px;
}
/*******************************  左侧菜单栏容器 End  **********************************/

/*******************************  右侧内容容器 start  ********************************/
#wraper_newsBody{
	background:url(../../image/contentBody/m06.png) 315px -135px no-repeat;
	_background:url(../../image/contentBody/m06.gif) 315px -135px no-repeat;
	width:667px;	
	float:left;
	min-height:400px;
	_height:400px;
	_height:auto !important;
}
