		$(".but_leave_message").bind('click',function(){
			//获取留言内容
			var myMessage = $(".textArea_leave_message").val();
			$.ajax({
				type: "POST",
			    url: "",
				data: "content=" + myMessage,
				success: function(msg){
					// attach the template			
					$("#ul_lastMessage").setTemplateElement("template");						
					// process the template			
					$("#ul_lastMessage").processTemplate(msg);	
				}
				});	
		});

})(initObject);
						
</script>
