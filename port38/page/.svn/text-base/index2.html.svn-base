<script type="text/javascript">
	function show()
	{
		document.getElementById("id_leave_message").style.display = "block";
		var content = "请填写留言内容，点击确定发送。";
		$(".textArea_leave_message").val(content).bind('click', function(){
			if($(".textArea_leave_message").val() == content) $(".textArea_leave_message").val("");
		});
	}
	function myhidden()
	{
		document.getElementById("id_leave_message").style.display = "none";		
	}
	
	/*名师风采中的图片滚动效果*/
	var speed=100;
	var tab=document.getElementById("slider");
	var tab1=document.getElementById("sliderOne");
	var tab2=document.getElementById("sliderTwo");
	tab2.innerHTML=tab1.innerHTML;
	function Marquee(){
	if(tab2.offsetWidth-tab.scrollLeft<=0)
	{
		tab.scrollLeft-=tab1.offsetWidth;
	}
	else{
		tab.scrollLeft++;
	}
	}
	var MyMar=setInterval(Marquee,speed);
	tab.onmouseover=function() {
		clearInterval(MyMar);
		};
	tab.onmouseout=function() {
		MyMar=setInterval(Marquee,speed);
		};


