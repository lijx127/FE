@charset "UTF-8";

/*整体容器*/
#wrapperAll{
	width:1000px;
	margin:0 auto;
	background:#FFF;
}
/*******************************  header容器 start  ********************************/
#wrapper_header{
	background:#fff;
	width:1000px;
	height:240px;	
	background:blue;
}
/*头部swf*/
#wrapper_swf{
	width:100%;
	height:173px;	
	background:url(../../image/index/header.jpg) left top no-repeat #fff;
}
/*头部导航栏*/
#wrapper_nav{
	width:100%;
	background:#3968b2;	
	height:67px;
	background:url(../../image/index/bg_nav.png) left top no-repeat;
	position:relative;
}
/*******************************  header容器 End  **********************************/

/*******************************  content容器 start  ********************************/
#wrapper_content{
	width:1000px;
	margin-top:11px;
}
/*左侧内容*/
#wrapper_contentLeft{
	width:280px;
	float:left;	
	margin-left:9px;
}
	/******************************* left 容器 start  ********************************/
	#wrapper_login{
		width:100%;
		height:180px;
		background:url(../../image/index/login.gif) left top no-repeat;
		position:relative;
	}
	#wrapper_video{
		width:100%;
		height:276px;
		margin-top:7px;	
		position:relative;
	}
	#wrapper_list{
		width:100%;
		height:800px;
		margin-top:10px;
		position:relative;
	}
	/******************************* left 容器 End  **********************************/
#wrapper_contentRight{
	width:695px;
	height:1271px;
	float:left;	
	margin-left:7px;
	position:relative;
}
/*******************************  content容器 End  **********************************/

/*******************************  footer容器 start  ********************************/
#wrapper_footer{
	background:url(../../image/index/bg_footer.jpg) repeat-x;
	width:1000px;
	height:80px;
	margin-top:17px;
	position:relative;	
}
/*******************************  footer容器 End  **********************************/